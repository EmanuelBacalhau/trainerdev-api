{
  "name": "trainer-dev-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node build/src/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --dir ./tests/unit",
    "test:integrations": "vitest run --dir ./tests/integrations",
    "test:e2e": "vitest run --dir ./tests/end-to-end",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "pretest:ui": "run-s test:create-prisma-environment test:install-prisma-environment",
    "pretest:coverage": "run-s test:create-prisma-environment test:install-prisma-environment",
    "pretest:integrations": "run-s test:create-prisma-environment test:install-prisma-environment",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx ./prisma/seeds/index.ts"
  },
  "keywords": [],
